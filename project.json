{
  "version": "1.0.0-*",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true,
    "additionalArguments": [
      "/ruleset:stylecop/ruleset.ruleset"
    ],
    "warningsAsErrors": true
  },
  "dependencies": {
    "StyleCop.Analyzers": {
      "version": "1.1.0-beta001",
      "type": "build"
    },
    "MSTest.TestFramework": "1.1.11",
    "dotnet-test-mstest": "1.1.2-preview"
  },
  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        }
      },
      "imports": "dnxcore50"
    }
  },
  "testRunner": "mstest"
}