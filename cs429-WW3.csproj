    <PackageReference Include="MSTest.TestFramework" Version="1.1.14" />
    <PackageReference Include="OpenTK.NETCore" Version="1.1.2571.6431" />
    <PackageReference Include="StyleCop.Analyzers" Version="1.0.1" />
    <None Include="maps.csv" CopyToOutputDirectory="Always" />
  </ItemGroup>
</Project>